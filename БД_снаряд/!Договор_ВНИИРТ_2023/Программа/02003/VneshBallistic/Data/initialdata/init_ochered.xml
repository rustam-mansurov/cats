            <description>Тип вооружения (1 - снаряды, 2 - ракеты)</description>
            <structure name="scalar">
              <datatype>integer</datatype>
              <unit></unit>
              <data>1</data>
            </structure>
          </var>
          <var name="sn_name">
            <description>Название снаряда</description>
            <structure name="scalar">
              <datatype>string</datatype>
              <unit></unit>
              <data>ОФ3-23</data>
            </structure>
          </var>
          <var name="sn_file">
            <description>Файл данных снаряда (/snaryad)</description>
            <structure name="scalar">
              <datatype>string</datatype>
              <unit></unit>
              <data>OF3-23.xml</data>
            </structure>
          </var>
          <var name="t0">
            <description>Начало очереди</description>
            <structure name="scalar">
              <datatype>double</datatype>
              <unit>с</unit>
              <data>0,1</data>
            </structure>
          </var>
          <var name="dt">
